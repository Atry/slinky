<idea-plugin>
    <id>slinky.core.intellij</id>
    <name>Slinky Library Support</name>
    <version>PATCH</version>
    <vendor url="http://www.jetbrains.com">JetBrains</vendor>

    <description>Type inference for the Slinky @react annotation</description>

    <idea-version since-build="PATCH" until-build="PATCH"/>

    <depends>org.intellij.scala</depends>

    <application-components></application-components>

    <project-components></project-components>

    <extensions defaultExtensionNs="org.intellij.scala">
        <syntheticMemberInjector implementation="slinky.core.SlinkyInjector"/>
    </extensions>

    <extensions defaultExtensionNs="com.intellij"></extensions>
</idea-plugin>
